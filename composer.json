{
    "name": "zikisacalesogaxa/dearapi",
    "description": "PHP Library for dear systems API v2 https://dearinventory.docs.apiary.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zikisa Sogaxa",
            "email": "zikisacalesogaxa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Cale\\Dear\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cale\\Dear\\Test\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    }
}
